{
    "name": "diablomedia/zf2-oauth2-server",
    "description": "ZF2 implementation of oauth2-server-php",
    "keywords": ["oauth", "oauth2", "auth", "zf2", "zendframework"],
    "type": "library",
    "license": "MIT",
    "authors":[
        {
            "name": "Ari Pringle",
            "email": "ari@diablomedia.com",
            "homepage": "https://diablomedia.com/"
        },
        {
            "name": "Jay Klehr",
            "email": "jay@diablomedia.com",
            "homepage": "https://diablomedia.com/"
        }
    ],
    "homepage": "https://github.com/diablomedia/zf2-oauth2-server",
    "require": {
        "php": ">=5.6.0",
        "bshaffer/oauth2-server-php": "^1.7",
        "diablomedia/oauth2-server-zendhttp-bridge": "^2.0.0",
        "zendframework/zend-form": "^2.0.0",
        "zendframework/zend-view": "^2.0.0",
        "zendframework/zend-mvc": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-0": {"OAuth2Server": "src/"},
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "scripts": {
        "fix-style": "php-cs-fixer fix",
        "check-style": "php-cs-fixer fix --dry-run -vv",
        "check-stan": "phpstan analyse -l 7 --autoload-file=vendor/autoload.php --memory-limit=768M src"
    }
}
